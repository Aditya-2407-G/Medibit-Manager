{
  "expo": {
    "name": "ProjectAppOrg",
    "slug": "ProjectAppOrg",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "packageName": "com.project.orgapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Medibit to access your camera",
          "microphonePermission": "Allow Medibit to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-document-picker",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(Medibit) to access your photos.",
          "savePhotosPermission": "Allow $(Medibit) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
